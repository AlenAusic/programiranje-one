#include <iostream>
using namespace std;

// Napisati program za izvrsavanje sljedeceg izraza:
// v = (z - y) / (2 * x - y * z)

int main() {
	int x, y, z;
	double v = 0;

	cout << "Unesite x: " << endl;
	cin >> x;
	cout << "Unesite y: " << endl;
	cin >> y;
	cout << "Unesite z: " << endl;
	cin >> z;

	if ((2 * x - y * z) < 0 || (2 * x - y * z) > 0) {
		v = (z - y) / (2 * x - y * z);
		cout << "Rjesenje iznosi: " << v << endl;
	}
	else
		cout << "Nazivnik je jednak nuli i zbog toga ovo nije moguce izracunati!" << endl;

	
	return 0;
	system("pause=0");
}
